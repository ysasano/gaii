#/bin/bash

info () {
  printf "\033[32m[INFO] $1\033[0m\n"
}

check_experiment () {
  info '実験ログのタイムスタンプ'
  poetry run python -u src/stat.py experiment.log
  echo ''

  info '直近の実験ログ'
  tail -n50 experiment.log
}

exec_experiment () {
  poetry run python -u src/gaii.py | tee experiment.log
}

setup () {
  # python 3.9.10インストール
  # poetry 1.1.11インストール
  poetry install
}

"$@"